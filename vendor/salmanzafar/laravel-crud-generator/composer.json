{
    "name": "salmanzafar/laravel-crud-generator",
    "description": "A simple Laravel library that allows you to create crud operations with a single command",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Salman Zafar",
            "email": "salmanzafar949@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "laravel5",
        "laravel-5",
        "laravel6",
        "laravel-6",
        "laravel7",
        "laravel-7",
        "laravel8",
        "laravel-8",
        "laravel-crud-generator",
        "laravel-api-crud-generator"
    ],
    "require": {
        "php": "^7.0 || ^7.1 || ^7.2 || ^7.3 || ^7.4"
    },
    "require-dev": {
        "laravel/framework": "^5.5 || ^6.0 || ^7.0 || ^8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Salman\\CrudGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Salman\\CrudGenerator\\CrudGeneratorServiceProvider"
            ],
            "aliases": {
                "CrudGenerator": "Salman\\CrudGenerator\\Facades\\CrudGenerator"
            }
        }
    }
}
